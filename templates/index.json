    "76f3ceeb-85f1-42aa-81cf-4d38e760cd9b": {
      "type": "custom-liquid",
      "custom_css": [
        "#typingText h1 {font-size: 62px; letter-spacing: 1px; \/* Espaciado entre letras *\/ line-height: 1.4; \/* Altura de línea *\/ text-align: center; \/* Alineación al centro *\/ margin: 0px;}",
        "#typingText span {border-right: 2px solid; white-space: nowrap; overflow: hidden;}",
        "#p {text-align: center;}"
      ],
      "settings": {
        "custom_liquid": "<div id=\"typingText\">\n  <h1>Somos \n<br><span><\/span><\/h1>\n<\/div>\n\n<script>\ndocument.addEventListener(\"DOMContentLoaded\", function() {\n  var texts = [\"LIFT\", \"e-commerce\"];\n  var typingElement = document.querySelector('#typingText span');\n  var typingDelay = 100;\n  var erasingDelay = 100;\n  var newTextDelay = 2000; \/\/ Tiempo entre cada texto\n  var textArrayIndex = 0;\n  var charIndex = 0;\n\n  function type() {\n    if (charIndex < texts[textArrayIndex].length) {\n      typingElement.textContent += texts[textArrayIndex].charAt(charIndex);\n      charIndex++;\n      setTimeout(type, typingDelay);\n    } \n    else {\n      setTimeout(erase, newTextDelay);\n    }\n  }\n\n  function erase() {\n    if (charIndex > 0) {\n      typingElement.textContent = texts[textArrayIndex].substring(0, charIndex-1);\n      charIndex--;\n      setTimeout(erase, erasingDelay);\n    } \n    else {\n      textArrayIndex = (textArrayIndex + 1) % texts.length;\n      setTimeout(type, typingDelay + 1100);\n    }\n  }\n\n  setTimeout(type, newTextDelay + 250);\n});\n<\/script>",
        "color_scheme": "accent-1",
        "padding_top": 20,
        "padding_bottom": 0
      }
    },
